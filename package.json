      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^9.0.1",
    "cross-env": "^7.0.3",
    "electron": "^32.1.0",
    "electron-is-dev": "^3.0.1",
    "wait-on": "^8.0.1",
    "webpack": "^5.94.0",
